{
  "name": "customers-demo-js",
  "version": "1.0.0",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "dev": "npm run db & webpack-dev-server --open --hot",
    "db": "json-server assets/customer-db.js",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:src": "eslint ./src/**/*.js",
    "lint:test": "eslint ./test/**/*.js",
    "doc": "jsdoc -d doc -P ./package.json -R ./README.md ./src/**/*.js",
    "test": "cross-env NODE_ENV=testing karma start test/config/karma.config.js"
  },
  "dependencies": {
    "angular": "1.6.4",
    "angular-ui-router": "0.4.2",
    "bootstrap": "4.0.0-alpha.6",
    "font-awesome": "4.7.0",
    "jquery": "3.2.1",
    "lodash": "4.17.4"
  },
  "devDependencies": {
    "angular-mocks": "1.6.4",
    "chai": "3.5.0",
    "cross-env": "5.0.0",
    "css-loader": "0.28.1",
    "eslint": "3.19.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-node": "4.2.2",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "expose-loader": "0.7.3",
    "file-loader": "0.11.1",
    "html-loader": "0.4.5",
    "istanbul-instrumenter-loader": "2.0.0",
    "jsdoc": "3.4.3",
    "json-server": "0.10.1",
    "karma": "1.7.0",
    "karma-chai": "0.1.0",
    "karma-chai-sinon": "0.1.5",
    "karma-chrome-launcher": "2.1.1",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-mocha": "1.3.0",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sinon": "1.0.5",
    "karma-source-map-support": "1.2.0",
    "karma-webpack": "2.0.3",
    "mocha": "3.4.1",
    "node-sass": "4.5.3",
    "phantomjs-prebuilt": "2.1.14",
    "sass-loader": "6.0.5",
    "sinon": "2.2.0",
    "sinon-chai": "2.10.0",
    "style-loader": "0.17.0",
    "url-loader": "0.5.8",
    "webpack": "2.5.1",
    "webpack-dev-server": "2.4.5"
  }
}
